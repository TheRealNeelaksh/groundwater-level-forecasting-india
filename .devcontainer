{
  "name": "Python Dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "python.defaultInterpreterPath": "/usr/local/bin/python3.10"
  },
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter",
    "ms-python.vscode-pylance"
  ],
  "postCreateCommand": "pip install -r requirements.txt",
  "remoteUser": "vscode"
}
